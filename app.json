{
  "expo": {
    "name": "SalaryPro",
    "slug": "salary-tracker-pro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#3E8ED0"
    },
    "jsEngine": "jsc",
    "ios": {
      "jsEngine": "jsc",
      "supportsTablet": true,
      "bundleIdentifier": "com.salarypro.app",
      "buildNumber": "1"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "13.4"
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "האפליקציה זקוקה לגישה לגלריה כדי לאפשר לך לצרף צילומי קבלות למשמרות."
        }
      ]
    ]
  }
}
